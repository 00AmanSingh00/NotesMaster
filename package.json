    "start:server": "cd Backend && pnpm dev",
    "start:client": "cd Frontend && pnpm start",
    "start:both": "concurrently \"pnpm start:server\" \"pnpm start:client\"",  
    "preview:client": "cd Frontend && pnpm build && pnpm serve --port 3001",
    "preview:server": "cd Backend && pnpm serve",
    "preview:both": "pnpm init:both && concurrently \"pnpm preview:server\" \"pnpm preview:client\"",
